{
    "compilerOptions": {
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictPropertyInitialization": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "moduleResolution": "node"
    },
    "include": [
      "girder_typings.d.ts",
      // ...other files or folders containing your TypeScript source code
    ],
    "exclude": [
      "node_modules"
    ]
  }
  